---
# -*- html -*- mode
topnav:
  - text: About
    href: index.html
  - text: Background
    href: background.html
  - text: Requirements
    href: requirements.html
  - text: License
    href: license.html
  - text: Download
    href: download.html
  - text: Configuration
    href: config.html
  - text: Changes
    href: changes.html
  - text: Support
    href: support.html
---
{% include header-default.html %}

    <!-- begin: main navigation #nav -->
    <div id="nav">
      <!-- skiplink anchor: navigation -->
      <a id="navigation" name="navigation"></a>
      <div class="hlist">
        <ul>
        {% for t in page.topnav %}
        {% if t.text == page.navname %}<li id="current">{% else %}<li>{% endif %}
        <a href="{{ t.href }}"{% if t.title %} title="{{ t.title }}"{% endif %}>{{ t.text }}</a>
        </li>
        {% endfor %}
        </ul>
      </div>
    </div>
    <!-- end: main navigation -->
    <!-- begin: main content area #main -->
    <div id="main">
      <!-- begin: #col1 static column -->
      <div id="col1">
        <div id="col1_content" class="clearfix">
          <!-- skiplink anchor: Content -->

        {% if page.sidenav != null %}
        {% for t in page.sidenav %}
        {% if t.head %}
        {% unless forloop.first %}</ul>{% endunless %}{% comment %}finish prev list{% endcomment %}
        <h6 class="vlist">{{ t.head }}</h6>
        <ul class="vlist">
        {% if forloop.length == 1 %}<li><!-- no items --></li>{% endif %}
        {% elsif t.active %}
        <li class="active"><strong>{{ t.text }}</strong></li>
        {% elsif t.href %}
        <li><a href="{{ t.href }}"{% if t.title %} title="{{ t.title }}"{% endif %}>{{ t.text }}</a></li>
        {% elsif t.text %}
        <li><strong>{{ t.text }}</strong></li>
        {% endif %}
        {% endfor %}
        </ul>{% comment %}finish prev list{% endcomment %}
        {% endif %}

{% include project-links.html %}


        </div>
      </div>
      <!-- end: #col1 -->
      <!-- begin: #col3 - first float column -->
      <div id="col3">
        <div id="col3_content" class="clearfix"> <a id="content" name="content"></a>
          <!-- skiplink anchor: Content -->

         {{ content }}


        </div>
        <div id="ie_clearing">&nbsp;</div>
        <!-- End: IE Column Clearing -->
      </div>
      <!-- end: #col3 -->
    </div>
    <!-- end: #main -->
{% include footer-default.html %}
